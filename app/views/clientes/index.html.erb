<p id="notice"><%= notice %></p>

<h1>Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Cpf</th>
      <th>Nome</th>
      <th>Data nasc</th>
      <th>Cep</th>
      <th>Logradouro</th>
      <th>Debito</th>
      <th>Num logradouro</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Uf</th>
      <th>Pais</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.cpf %></td>
        <td><%= cliente.nome %></td>
        <td><%= cliente.data_nasc %></td>
        <td><%= cliente.cep %></td>
        <td><%= cliente.logradouro %></td>
        <td><%= cliente.debito %></td>
        <td><%= cliente.num_logradouro %></td>
        <td><%= cliente.bairro %></td>
        <td><%= cliente.cidade %></td>
        <td><%= cliente.uf %></td>
        <td><%= cliente.pais %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
